<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="234.0" prefWidth="495.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.deluxepter.rlmaploader.controller.SettingsController">
    <children>
        <TitledPane alignment="CENTER" animated="false" collapsible="false" layoutX="24.0" layoutY="24.0"
                    prefHeight="181.0" prefWidth="448.0" text="%common.general">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="154.0" prefWidth="446.0">
                    <children>
                        <TextField fx:id="rldirTextField" layoutX="15.0" layoutY="38.0" prefHeight="26.0"
                                   prefWidth="383.0"/>
                        <Label layoutX="15.0" layoutY="14.0" text="%common.rldir.directory"/>
                        <Button layoutX="403.0" layoutY="38.0" mnemonicParsing="false" onAction="#setRlDirectory"
                                prefHeight="26.0" prefWidth="28.0" text="..."/>
                        <Label layoutX="15.0" layoutY="81.0" text="%common.theme"/>
                        <ComboBox fx:id="themeComboBox" layoutX="15.0" layoutY="105.0" onAction="#setTheme"
                                  prefWidth="150.0"/>
                        <Label layoutX="189.0" layoutY="81.0" text="%common.language"/>
                        <ComboBox fx:id="languageComboBox" layoutX="189.0" layoutY="105.0" onAction="#setLanguage"
                                  prefWidth="150.0"/>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
